<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:layout_marginTop="20dp"
    android:background="@color/background_white"
    android:id="@+id/wellinfo_deviceStatusTitle"
    >
    <include layout="@layout/line_gray"/>
    <ImageView
        android:id="@+id/fun_image_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/common_drawablePadding15"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:src="@mipmap/wellinfo_devicestatus_logo"
        />
    <TextView
        android:id="@+id/wellinfo_wellMapName"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/common_drawablePadding15"
        android:layout_toRightOf="@id/fun_image_left"
        android:text="设备状态"
        style="@style/viewtext16_style"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:paddingRight="@dimen/common_drawablePadding15"
        android:layout_centerVertical="true"
        android:src="@mipmap/right_arrow_gray"
        />


</RelativeLayout>
