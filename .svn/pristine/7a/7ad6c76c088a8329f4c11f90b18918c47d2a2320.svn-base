<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0eff5"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/warn_ds_title"
        android:layout_width="match_parent"
        android:background="@drawable/shape_top2_green"
        android:orientation="horizontal"
        android:layout_height="44dp">
        <TextView
            android:id="@+id/warn_ds_titleWellName"
            android:layout_width="220dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:gravity="left|center_vertical"
            android:layout_alignParentLeft="true"
            android:textColor="@color/white"
            android:text="新疆建设兵团第二队第三支队第五班直辖出三里弄08号井"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/warn_ds_time"
            android:layout_width="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="5dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:singleLine="true"
            android:layout_toRightOf="@+id/tvw_well_info"
            android:textColor="@color/white"
            android:gravity="right|center_vertical"
            android:text="1月12日 11:22"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/warn_ds_titleWellNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:gravity="right|center_vertical"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="5dp"
            android:singleLine="true"
            android:layout_below="@+id/tvw_warn_occur_time"
            android:layout_toRightOf="@+id/tvw_well_info"
            android:textColor="@color/white"
            android:text="编码17032811121000004"
            android:textSize="12sp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/common_drawablePadding5"
        android:paddingTop="@dimen/common_drawablePadding5"
        android:background="@color/white"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:orientation="horizontal"
            android:layout_marginLeft="5dp"
            android:layout_height="22dp">
            <TextView
                android:id="@+id/warn_ds_rtuTi"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:paddingRight="@dimen/common_drawablePadding5"
                android:gravity="center_vertical"
                android:background="#999999"
                android:textColor="@color/white"
                android:text="RTU"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/warn_ds_rtuStateType"
                android:layout_width="wrap_content"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|left"
                android:textColor="#64afe6"
                android:layout_height="match_parent"
                android:text="4"
                android:textSize="14sp" />
            <TextView
                android:layout_width="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|right"
                android:paddingRight="5dp"
                android:textColor="#999999"
                android:layout_height="match_parent"
                android:text="查看详情"
                android:drawableRight="@mipmap/right_arrow_gray"
                android:textSize="14sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/common_drawablePadding3"
            android:layout_marginLeft="5dp"
            android:layout_height="22dp">
            <TextView
                android:id="@+id/warn_ds_flowMeTi"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:paddingRight="@dimen/common_drawablePadding5"
                android:gravity="center_vertical"
                android:background="#999999"
                android:textColor="@color/white"
                android:text="流量计"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/warn_ds_flowMeStateType"
                android:layout_width="wrap_content"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|left"
                android:textColor="#64afe6"
                android:layout_height="match_parent"
                android:text=" "
                android:textSize="14sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:orientation="horizontal"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="@dimen/common_drawablePadding3"
            android:layout_height="22dp">
            <TextView
                android:id="@+id/warn_ds_electMeTi"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:paddingRight="@dimen/common_drawablePadding5"
                android:gravity="center_vertical"
                android:background="#999999"
                android:textColor="@color/white"
                android:text="电表"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/warn_ds_electMeStateType"
                android:layout_width="wrap_content"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|left"
                android:textColor="#64afe6"
                android:layout_height="match_parent"
                android:text=" "
                android:textSize="14sp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
