<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent"
    android:orientation="vertical" >
    <LinearLayout
        android:id="@+id/pump1_input_page"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:orientation="vertical"
        android:gravity="center">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/common_drawablePadding50"
        android:background="@color/transparent"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"

            android:text="确认阀门已打开?否则会造成设备损坏!"
            android:textSize="@dimen/text15"
            android:textColor="@color/text_color_white"/>
        <TextView
            android:layout_marginTop="@dimen/common_drawablePadding12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="@color/text_color_white"
            android:textSize="@dimen/text20"
            android:textStyle="bold"
            android:text="请输入密码"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/etPwdOne_setLockPwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdTwo_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdThree_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdFour_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdFive_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdSix_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdSeven_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <ImageView
            android:id="@+id/etPwdEight_setLockPwd"
            android:layout_marginLeft="@dimen/common_drawablePadding15.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/input_pw_round"
            android:gravity="center">
        </ImageView>
        <EditText
            android:id="@+id/etPwdText_setLockPwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_drawablePadding5"
        android:gravity="center"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/pump1_pw_err"
            android:layout_marginTop="@dimen/common_drawablePadding5.5"
            android:visibility="invisible"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/text_color_red"
            android:background="#00000000"
            android:textSize="@dimen/text14"
            android:text="密码错误,请重新输入!"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_drawablePadding20"
        android:gravity="center"
        android:orientation="vertical" >
        <android.inputmethodservice.KeyboardView
            android:id="@+id/keyboard_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/transparent"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:keyBackground="@drawable/key0_btn_press"
            android:keyTextSize="33sp"
            android:keyTextColor="@color/white" />
    </LinearLayout>
    </LinearLayout>
    <!--等待界面-->
    <LinearLayout
        android:id="@+id/pump1_wait_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        android:gravity="center">
        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="请稍等!"
            android:textSize="@dimen/text20"
            android:textColor="@color/text_color_white"/>
       <!-- <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_drawablePadding17.5"
            android:src="@mipmap/pump_progess"/>-->
        <TextView
            android:id="@+id/pump1_onOffResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:text=""/>
        <ProgressBar
            style="@style/progressbar_style" />
    </LinearLayout>
    <!--成功界面-->
    <LinearLayout
        android:id="@+id/pump1_success_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        android:gravity="center">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="成功!"
            android:textSize="@dimen/text20"
            android:textColor="@color/text_color_white"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_drawablePadding17.5"
            android:src="@mipmap/pump_success"/>
    </LinearLayout>
    <!--失败界面-->
    <LinearLayout
        android:id="@+id/pump1_fail_page"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone"
        android:gravity="center">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="失败!"
            android:textSize="@dimen/text20"
            android:textColor="@color/text_color_white"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_drawablePadding17.5"
            android:src="@mipmap/pump_fail"/>
    </LinearLayout>

</LinearLayout>