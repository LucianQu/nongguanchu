<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_title"
        style="@style/tab_titlebar">

        <LinearLayout
            android:layout_width="54dp"
            android:layout_height="match_parent"
            android:onClick="onReturnClick"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imgbtn_back"
                style="@style/TitleBack"
                android:layout_gravity="center_vertical"
                android:focusable="false" />
        </LinearLayout>

        <TextView
            android:id="@+id/wellinfo_tvw_title"
            style="@style/TitleText"
            android:text="报警消息" />


    </RelativeLayout>

    <include
        android:id="@+id/ll_warn1"
        layout="@layout/item_message_warn"></include>

    <include
        android:id="@+id/ll_warn2"
        layout="@layout/item_message_warn"></include>

    <include
        android:id="@+id/ll_warn3"
        layout="@layout/item_message_warn"></include>

    <include
        android:id="@+id/ll_warn4"
        layout="@layout/item_message_warn"></include>

    <include
        android:id="@+id/ll_warn5"
        layout="@layout/item_message_warn"></include>


</LinearLayout>
