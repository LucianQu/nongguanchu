<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- 标题relativelayout -->
    <style name="tab_titlebar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@color/titlebar_green</item>
    </style>
    <!-- 标题textview -->
    <style name="TitleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">20sp</item>
    </style>
    <!-- 返回 -->
    <style name="TitleBack">
        <item name="android:layout_width">7.5dp</item>
        <item name="android:layout_height">12.5dp</item>
        <item name="android:background">@mipmap/ic_back</item>
        <item name="android:layout_marginLeft">9dp</item>
    </style>
    <!-- title右侧 -->
    <style name="Titleright">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@mipmap/ic_add_more</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!--高德地图主题-->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
