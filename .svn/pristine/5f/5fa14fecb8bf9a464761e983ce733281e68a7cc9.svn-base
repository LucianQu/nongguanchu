<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0eff5"
    android:orientation="vertical">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_marginTop="10dp"
    android:layout_marginRight="39dp"
    android:layout_marginLeft="39dp"
    android:layout_height="40dp">
    <include
        android:id="@+id/ll_select_year_month"
        layout="@layout/item_key_vlue_listview"></include>
</LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp">

        <LinearLayout
            android:id="@+id/ll_select_type"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_weight="1"
            android:background="@drawable/shape_4_white"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvw_spinner_wcun"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="2017å¹´"
                android:textColor="@color/black" />

            <TextView
                android:layout_width="7.5dp"
                android:layout_height="5dp"
                android:layout_gravity="center_vertical"
                android:background="@mipmap/ic_down_more" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_weight="2.3"
            android:orientation="vertical">

            <include
                android:id="@+id/ll_useWater"
                layout="@layout/item_key_vlue_listview"
                android:layout_width="match_parent"
                android:layout_height="20dp"></include>

            <include
                android:id="@+id/ll_useElec"
                layout="@layout/item_key_vlue_listview"
                android:layout_width="match_parent"
                android:layout_height="20dp"></include>

            <include
                android:id="@+id/waterElecValue"
                layout="@layout/item_key_vlue_listview"
                android:layout_width="match_parent"
                android:layout_height="20dp"></include>
        </LinearLayout>
    </LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_marginLeft="10dp"
    android:layout_marginBottom="10dp"
    android:layout_marginRight="10dp"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_weight="1">
        <include
            android:id="@+id/chart_use_water"
            layout="@layout/ll_hellochart"></include>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_weight="1">
        <include
            android:id="@+id/chart_use_elec"
            layout="@layout/ll_hellochart"></include>
    </LinearLayout>
</LinearLayout>
</LinearLayout>
