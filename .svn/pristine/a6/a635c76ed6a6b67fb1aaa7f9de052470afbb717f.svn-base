<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0eff5"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/warn_ds_title"
        android:layout_width="match_parent"
        android:background="@drawable/shape_top2_green"
        android:orientation="horizontal"
        android:layout_height="22dp">
        <TextView
            android:id="@+id/warn_ds_titleWellNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/common_drawablePadding10"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:textColor="@color/white"
            android:text="机井编号"
            android:textSize="14sp"/>
        <ImageView
            android:id="@+id/warn_ds_titleIme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/common_drawablePadding20"
            android:layout_centerVertical="true"
            android:src="@mipmap/right_arrow_gray"/>
        <TextView
            android:id="@+id/warn_ds_titleDetails"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/common_drawablePadding5"
            android:layout_toLeftOf="@id/warn_ds_titleIme"
            android:textColor="@color/white"
            android:text="详情"
            android:textSize="14sp"/>
        <TextView
            android:id="@+id/warn_ds_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/common_drawablePadding10"
            android:layout_toLeftOf="@id/warn_ds_titleDetails"
            android:textColor="@color/white"
            android:text="1月15日 11:22"
            android:textSize="14sp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/common_drawablePadding5"
        android:paddingTop="@dimen/common_drawablePadding5"
        android:background="@color/white"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:orientation="horizontal"
            android:layout_marginLeft="5dp"
            android:layout_height="22dp">
            <TextView
                android:id="@+id/warn_ds_rtuTi"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:paddingRight="@dimen/common_drawablePadding5"
                android:gravity="center_vertical"
                android:background="#999999"
                android:textColor="@color/white"
                android:text="RTU"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/warn_ds_rtuStateType"
                android:layout_width="wrap_content"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|left"
                android:textColor="#64afe6"
                android:layout_height="match_parent"
                android:text=" "
                android:textSize="14sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/common_drawablePadding3"
            android:layout_marginLeft="5dp"
            android:layout_height="22dp">
            <TextView
                android:id="@+id/warn_ds_flowMeTi"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:paddingRight="@dimen/common_drawablePadding5"
                android:gravity="center_vertical"
                android:background="#999999"
                android:textColor="@color/white"
                android:text="流量计"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/warn_ds_flowMeStateType"
                android:layout_width="wrap_content"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|left"
                android:textColor="#64afe6"
                android:layout_height="match_parent"
                android:text=" "
                android:textSize="14sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            android:orientation="horizontal"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="@dimen/common_drawablePadding3"
            android:layout_height="22dp">
            <TextView
                android:id="@+id/warn_ds_electMeTi"
                android:layout_width="55dp"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:paddingRight="@dimen/common_drawablePadding5"
                android:gravity="center_vertical"
                android:background="#999999"
                android:textColor="@color/white"
                android:text="电表"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/warn_ds_electMeStateType"
                android:layout_width="wrap_content"
                android:paddingLeft="@dimen/common_drawablePadding5"
                android:gravity="center|left"
                android:textColor="#64afe6"
                android:layout_height="match_parent"
                android:text=" "
                android:textSize="14sp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
