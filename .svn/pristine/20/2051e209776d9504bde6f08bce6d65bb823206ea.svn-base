<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/wellinfo_linearlayout_match_vertical_style"
    android:background="@color/background_gray">

    <!--Title Start-->
    <LinearLayout
        android:layout_height="90dp"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="@color/background_white">
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >
            <ImageView
                android:layout_gravity="center_horizontal|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/wellinfo_title"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:paddingLeft="@dimen/common_drawablePadding5"
            android:gravity="center_vertical"
            android:orientation="vertical">
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style">
                <TextView
                    android:text="@string/wellNo"
                    style="@style/wellinfo_viewtext_darkgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_No"
                    android:text="88888888"
                    style="@style/wellinfo_viewtext_black_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style">
                <TextView
                    android:text="@string/wellName"
                    style="@style/wellinfo_viewtext_darkgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_Name"
                    android:text="奎屯1兵团1号井"
                    style="@style/wellinfo_viewtext_black_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style">
                <TextView
                    android:text="@string/wellUser"
                    style="@style/wellinfo_viewtext_darkgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_User"
                    android:text="苏神_神一样的存在"
                    style="@style/wellinfo_viewtext_black_style"
                    />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/line1"/>
    <!--Title End-->

    <include
        layout="@layout/wellinfo_well"/>

    <include layout="@layout/line1"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="@color/background_gray"></LinearLayout>

    <include layout="@layout/line1"/>
    <!--剩余水量,电量,开关泵页面 Start  -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:layout_marginTop="0.1dp"
        android:orientation="horizontal"
        android:background="@color/background_white">

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:paddingLeft="@dimen/common_drawablePadding5"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style">
                <ImageView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/wellinfo_watersurplus"/>
                <TextView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:text="@string/waterSurplus"
                    android:textColor="@color/text_color_blue"
                    style="@style/wellinfo_viewtext_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_WaterSurplus"
                    android:textColor="@color/text_color_blue"
                    android:text="666吨"
                    style="@style/wellinfo_viewtext_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style"
                android:layout_marginTop="@dimen/common_drawablePadding5">
                <ImageView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/wellinfo_electsurplus"/>
                <TextView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:text="@string/waterSurplus"
                    android:textColor="@color/text_color_pink"
                    style="@style/wellinfo_viewtext_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_electSurplus"
                    android:textColor="@color/text_color_pink"
                    android:text="888度"
                    style="@style/wellinfo_viewtext_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style"
                android:layout_marginTop="@dimen/common_drawablePadding10">
                <TextView
                    android:text="@string/yearElecConsum"
                    style="@style/wellinfo_viewtext_lightgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_yearElectConsum"
                    android:text="6666度"
                    style="@style/wellinfo_viewtext_lightgray_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style">
                <TextView
                    android:text="@string/yearWaterConsum"
                    style="@style/wellinfo_viewtext_lightgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_yearWaterConsum"
                    android:text="7777吨"
                    style="@style/wellinfo_viewtext_lightgray_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style">
                <TextView
                    android:text="@string/yearPlanWaterConsum"
                    style="@style/wellinfo_viewtext_lightgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_yearPlanWaterConsum"
                    android:text="8888吨"
                    style="@style/wellinfo_viewtext_lightgray_style"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingRight="@dimen/common_drawablePadding15"
            >
            <ImageView
                android:layout_gravity="center_horizontal|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/wellinfo_pumpopen"/>
        </LinearLayout>
    </LinearLayout>
    <!--剩余水量,电量,开关泵页面 End  -->
    <include layout="@layout/line1"/>
    <include
        layout="@layout/wellinfo_devicestatus"/>
    <include
        layout="@layout/line1"/>
    <!--设备状态 start-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0.1dp"
        android:background="@color/background_gray"
        android:id="@+id/wellinfo_deviceStatusContent"
        android:gravity="top"
        android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_drawablePadding10"
                android:orientation="horizontal">
                <ImageView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/wellinfo_devicestatus_red"/>
                <TextView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:text="@string/deviceStatusRtu"
                    style="@style/wellinfo_viewtext_red_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_deviceStatusRtu"
                    android:text="正常/RTU掉电"
                    style="@style/wellinfo_viewtext_red_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style"
                android:layout_marginTop="@dimen/common_drawablePadding5">
                <ImageView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@mipmap/wellinfo_devicestatus_green"/>
                <TextView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:text="@string/deviceStatusFlowmeter"
                    style="@style/wellinfo_viewtext_darkgray_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_deviceStatusFlowMeter"
                    android:text="正常/故障/流量计电压低"
                    style="@style/wellinfo_viewtext_darkgray_style"
                    />
            </LinearLayout>
            <LinearLayout
                style="@style/wellinfo_linearlayout_wrap_horizontal_style"
                android:layout_marginTop="@dimen/common_drawablePadding5">
                <ImageView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@mipmap/wellinfo_devicestatus_red"/>
                <TextView
                    android:paddingLeft="@dimen/common_drawablePadding10"
                    android:text="@string/deviceStatusFlowmeter"
                    style="@style/wellinfo_viewtext_red_style"
                    />
                <TextView
                    android:id="@+id/wellInfo_deviceStatusElectMeter"
                    android:text="正常/故障"
                    style="@style/wellinfo_viewtext_red_style"
                    />
            </LinearLayout>


    </LinearLayout>
    <!--设备状态 end-->

</LinearLayout>
