<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/linearlayout_match_vertical_style"
    android:background="@color/background_gray"
    >
    <!--RTU start-->
    <LinearLayout
        android:id="@+id/ds_rtuTitleBackground"
        style="@style/devicestatus_imageview_h22dp_style"
        android:background="@drawable/ds_title_background_green"
        >
        <TextView
            android:id="@+id/ds_rtuTitle"
            style="@style/layout_width_height_wrap_style"
            android:paddingLeft="@dimen/common_drawablePadding15"
            android:textSize="@dimen/text14"
            android:textColor="@color/background_white"
            android:text=" "/>
    </LinearLayout>

    <LinearLayout
        style="@style/devicestatus_imageview_vWrap_style"
        android:background="@drawable/devicestatus_titlecontent_background"
        android:paddingBottom="@dimen/common_drawablePadding5"
        >
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/rtuNo"/>
            <TextView
                android:id="@+id/ds_rtuNo"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/rtuManuFactures"/>
            <TextView
                android:id="@+id/ds_rtuFactures"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/rtuModel"/>
            <TextView
                android:id="@+id/ds_rtuModel"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style"
            android:paddingTop="@dimen/common_drawablePadding5"
            android:layout_marginLeft="@dimen/common_drawablePadding15"
            android:layout_marginRight="@dimen/common_drawablePadding15">
            <TextView
                android:id="@+id/ds_rtuFaultInfo"
                style="@style/layout_width_height_wrap_style"
                android:visibility="gone"
                android:textColor="@color/text_color_red"
                android:ellipsize="none"
                android:maxLines="3"
                android:inputType="textMultiLine"
                android:text=" "/>
        </LinearLayout>
    </LinearLayout>
    <!--RTU          end-->
    <!--FlowMeter          start-->
    <LinearLayout
        android:id="@+id/ds_flowMTitleBackground"
        style="@style/devicestatus_imageview_h22dp_style"
        android:background="@drawable/ds_title_background_green"
        >
        <TextView
            android:id="@+id/ds_flowMeterTitle"
            style="@style/layout_width_height_wrap_style"
            android:paddingLeft="@dimen/common_drawablePadding15"
            android:textSize="@dimen/text14"
            android:textColor="@color/background_white"
            android:text=" "/>
    </LinearLayout>

    <LinearLayout
        style="@style/devicestatus_imageview_vWrap_style"
        android:background="@drawable/devicestatus_titlecontent_background"
        android:paddingBottom="@dimen/common_drawablePadding5"
        >
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/flowMeterNo"/>
            <TextView
                android:id="@+id/ds_flowMeterNo"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/flowMeterFactures"/>
            <TextView
                android:id="@+id/ds_flowMeterFactures"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/flowMeterModel"/>
            <TextView
                android:id="@+id/ds_flowMeterModel"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style"
            android:paddingTop="@dimen/common_drawablePadding5"
            android:layout_marginLeft="@dimen/common_drawablePadding15"
            android:layout_marginRight="@dimen/common_drawablePadding15">
            <TextView
                android:id="@+id/ds_flowMeterFaultInfo"
                style="@style/layout_width_height_wrap_style"
                android:visibility="gone"
                android:textColor="@color/text_color_red"
                android:ellipsize="none"
                android:maxLines="3"
                android:inputType="textMultiLine"
                android:text=" "/>
        </LinearLayout>
    </LinearLayout>
    <!--FlowMeter          end-->
    <!--ElectMeter          start-->
    <LinearLayout
        android:id="@+id/ds_electMTitleBackground"
        style="@style/devicestatus_imageview_h22dp_style"
        android:background="@drawable/ds_title_background_green"
        >
        <TextView
            android:id="@+id/ds_electMeterTitle"
            style="@style/layout_width_height_wrap_style"
            android:paddingLeft="@dimen/common_drawablePadding15"
            android:textSize="@dimen/text14"
            android:textColor="@color/background_white"
            android:text=" "/>
    </LinearLayout>

    <LinearLayout
        style="@style/devicestatus_imageview_vWrap_style"
        android:background="@drawable/devicestatus_titlecontent_background"
        android:paddingBottom="@dimen/common_drawablePadding5"
        >
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/electMeterNo"/>
            <TextView
                android:id="@+id/ds_electMeterNo"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/electMeterFactures"/>
            <TextView
                android:id="@+id/ds_electMeterFactures"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style">
            <TextView
                style="@style/viewtext14_white_style"
                android:background="@drawable/devicestatus_text_background"
                android:text="@string/electMeterModel"/>
            <TextView
                android:id="@+id/ds_electMeterModel"
                style="@style/viewtext14_gray_style"
                android:layout_marginLeft="@dimen/common_drawablePadding5"
                android:text=" "/>
        </LinearLayout>
        <LinearLayout
            style="@style/layout_width_height_wrap_marginleft5_style"
            android:paddingTop="@dimen/common_drawablePadding5"
            android:layout_marginLeft="@dimen/common_drawablePadding15"
            android:layout_marginRight="@dimen/common_drawablePadding15">
            <TextView
                android:id="@+id/ds_electMeterFaultInfo"
                style="@style/layout_width_height_wrap_style"
                android:visibility="gone"
                android:textColor="@color/text_color_red"
                android:ellipsize="none"
                android:maxLines="3"
                android:inputType="textMultiLine"
                android:text=" "/>
        </LinearLayout>
    </LinearLayout>
    <!--ElectMeter          end-->
</LinearLayout>
